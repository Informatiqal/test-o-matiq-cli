{
  "name": "test-o-matiq-cli",
  "version": "0.0.3",
  "description": "CLI wrapper for test-o-matiq. Data tests for Qlik Sense apps ",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "preferGlobal": true,
  "bin": {
    "test-o-matiq": "./bin/test-o-matiq.js"
  },
  "scripts": {
    "start": "node dist/app.js",
    "prestart": "npm run build-tsc",
    "build-tsc": "tsc --p tsconfig.debug.json",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "debug": "tsc --sourcemap",
    "test": "set TS_NODE_PROJECT=tsconfig.test.json&mocha --require ts-node/register test/**/*.spec.ts",
    "docs": "npx typedoc src/index.doc.ts --name \"Automatiqal\" --excludePrivate --hideGenerator"
  },
  "homepage": "https://github.com/informatiqal/test-o-matiq-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/informatiqal/test-o-matiq-cli.git"
  },
  "keywords": [
    "qlik",
    "sense",
    "build",
    "script",
    "reload"
  ],
  "license": "MIT",
  "dependencies": {
    "@p-mcgowan/minimist": "^2.1.0",
    "enigma-mixin": "^1.4.0",
    "enigma.js": "^2.12.0",
    "js-yaml": "^4.1.0",
    "ora": "^7.0.1",
    "test-o-matiq": "^0.4.2",
    "ws": "^8.14.2"
  },
  "engines": {
    "node": ">=12.20"
  },
  "devDependencies": {
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/chai": "4.3.6",
    "@types/chalk": "2.2.0",
    "@types/enigma.js": "2.2.3",
    "@types/js-yaml": "4.0.6",
    "@types/mocha": "10.0.2",
    "@types/node": "20.6.5",
    "@types/qlik-engineapi": "12.67.14",
    "@types/ws": "8.5.5",
    "ajv": "^8.12.0",
    "chai": "4.3.8",
    "dotenv": "16.3.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "rollup": "3.29.3",
    "rollup-plugin-delete": "2.0.0",
    "ts-node": "10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.1",
    "typescript": "^5.0.0"
  }
}

    "qlik",
    "sense",
    "build",
    "script",
    "reload"
  ],
  "license": "MIT",
  "dependencies": {
    "@p-mcgowan/minimist": "^2.1.0",
    "enigma-mixin": "^1.3.2",
    "enigma.js": "^2.9.0",
    "js-yaml": "^4.1.0",
    "ora": "^6.1.2",
    "ws": "^8.8.1"
  },
  "engines": {
    "node": ">=12.20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",

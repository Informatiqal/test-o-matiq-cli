    "@types/node": "18.7.9",
    "@types/qlik-engineapi": "12.67.12",
    "@types/ws": "8.5.3",
    "ajv": "8.11.0",
    "chai": "4.3.6",
    "dotenv": "16.0.3",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "rollup": "2.79.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.34.0",
    "ts-node": "10.9.1"
